{
  "name": "@aproxima/eslint-config",
  "version": "0.0.4",
  "description": "ESLint configuration for Aproxima",
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "eslint-config",
    "eslintconfig",
    "lint",
    "config"
  ],
  "author": "Andre Landgraf <andre@aproxima.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aproxima-tech/aproxima.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    },
    "./remix": {
      "import": "./remix.js",
      "require": "./remix.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0",
    "prettier": ">=3.2.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript-eslint": "^7.0.2"
  }
}
