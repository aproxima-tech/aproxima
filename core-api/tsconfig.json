{
  "include": ["**/*.ts", "**/*.tsx"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types",
      "vitest/globals",
      "vitest-environment-miniflare/globals"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}